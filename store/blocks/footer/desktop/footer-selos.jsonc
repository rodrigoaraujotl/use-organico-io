{
    "flex-layout.row#footer-selos": {
        "title": "Selos pagamento e certificados - Container",
        "children": [
            "flex-layout.col#footer-formas-pagamento",
            "flex-layout.col#footer-selos-certifications",
            "flex-layout.col#footer-selos-security"
        ],
        "props": {
            "colSizing": "auto",
            "blockClass": "footer-selos"
        }
    },
    "flex-layout.col#footer-formas-pagamento": {
        "title": "Formas de Pagamento - Container",
        "children": [
            "rich-text#footer-formas-pagamento-title",
            "image#footer-formas-pagamento-image"
        ],
        "props": {
            "preventVerticalStretch": true,
            "blockClass": ["footer-menus", "footer-selos-payment"]
        }
    },
    "rich-text#footer-formas-pagamento-title": {
        "props": {
            "blockClass": ["footer-titles", "footer-bottom-titles"],
            "text": "Formas de Pagamento"
        }
    },
    "image#footer-formas-pagamento-image": {
        "props": {
          "src": "/arquivos/bandeiras-footer.png",
          "maxHeight": 68
        }
      },
    "flex-layout.col#footer-selos-payment": {
        "title": "Formas de Pagamento - Container",
        "children": [
            "list-context.image-list#footer-selos-payment"
        ],
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "footer-selos-payment"
        }
    },
    
    "list-context.image-list#footer-selos-payment": {
        "children": [
            "list-context-renderer#footer-selos"
        ],
        "props": {
            "height": 31,
            "images": [
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-amex-new-footer.png?v=637972222250530000",
                    "description": "American Express"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-elo-new-footer.png?v=637972222254000000",
                    "description": "Elo"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-visa-new-footer.png?v=637972222264330000",
                    "description": "Visa"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-mastercard-new-footer.png?v=637972222257430000",
                    "description": "Mastercard"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-dinners-new-footer.png?v=637972222252270000",
                    "description": "Dinners"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-boleto-new-footer.png?v=637972222250700000",
                    "description": "Boleto"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-pix-new-footer.png?v=637972222261200000",
                    "description": "Pix"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-visa-checkout-new-footer.png?v=637972222262770000",
                    "description": "Visa Checkout"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-hipercard-new-footer.png?v=637972222255700000",
                    "description": "Hipercard"
                },
                {
                    "image": "https://iouseorganico.vteximg.com.br/arquivos/logo-nubank-new-footer.png?v=637972222259000000",
                    "description": "Nubank"
                }
            ],
            "blockClass": "footer-selos"
        }
    },
    "list-context-renderer#footer-selos": {
        "props": {
            "listElement": "div",
            "itemElement": "div",
            "blockClass": "footer-selos"
        }
    },
    "flex-layout.col#footer-selos-security": {
        "title": "Segurança - Container",
        "children": [
            "rich-text#footer-security",
            "list-context.image-list#footer-security"
        ],
        "props": {
            "preventVerticalStretch": true,
            "blockClass": ["footer-menus", "footer-selos-security"]
        }
    },
    "rich-text#footer-security": {
        "title": "Título - Segurança",
        "props": {
            "text": "Loja segura",
            "blockClass": ["footer-bottom-titles", "footer-titles"]
