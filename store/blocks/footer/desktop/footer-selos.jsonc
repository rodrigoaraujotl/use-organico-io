        }
    },
    "flex-layout.col#footer-selos-social-media": {
        "title": "Medias Sociais - Container",
        "children": [
            "flex-layout.row#container-social-media-icons"
        ],
        "props": {
            "preventVerticalStretch": true,
            "blockClass": "footer-selos-social-media"
        }
    },
    "flex-layout.row#container-social-media-icons":{
        "props":{
            "preventHorizontalStretch":true,
            "blockClass":"container-social-media-icons"
        },
        "children":[
            "image#facebook-icon",
            "image#instagram-icon",
            "image#youtube-icon"
        ]
    },
    "image#facebook-icon":{
        "props":{
            "src":"https://iouseorganico.vteximg.com.br/arquivos/u-footer-newfacebook.png",
            "alt":"",
            "link": {
                "url": "https://www.facebook.com/useorganico/",
                "openNewTab": "true",
                "attributeNofollow":true
            },
            "blockClass":"social-media-icon"
        }
    },
    "image#instagram-icon":{
        "props":{
            "src":"https://iouseorganico.vteximg.com.br/arquivos/u-footer-newinstagram.png",
            "alt":"",
            "link": {
                    "url": "https://www.instagram.com/useorganico/",
                    "openNewTab": "true",
                    "attributeNofollow":true
                },
            "blockClass":"social-media-icon"
        }
    },
    "image#youtube-icon":{
        "props":{
            "src":"https://iouseorganico.vteximg.com.br/arquivos/u-footer-newyoutube.png",
            "alt":"",
            "link": {
                "url": "https://www.youtube.com/useorganico/",
                "openNewTab": "true",
                "attributeNofollow":true
            },
            "blockClass":"social-media-icon"
        }
    }
}